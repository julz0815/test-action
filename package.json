{
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.0",
    "@fastify/sensible": "^5.2.0",
    "@prisma/client": "^4.9.0",
    "dotenv": "^16.0.1",
    "fastify": "^4.14.1",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "help": "^3.0.2",
    "octokit": "^2.0.14"
  },
  "devDependencies": {
    "@octokit/types": "^8.0.0",
    "@tsconfig/node16": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.9.0",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "semantic-release": "^21.0.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "semantic-release-major-tag": "^0.3.0"
  },
  "scripts": {
    "start": "npm run build:ts && fastify start -l info dist/app.js -a 0.0.0.0 -p 3001",
    "start:prod": "fastify start -l info app.js -a 0.0.0.0 -p 3001",
    "build": "npx tsc",
    "clean": "rm -rf dist",
    "postinstall": "prisma generate",
    "test": "npm run build:ts && tsc -p tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "build:ts": "npx tsc",
    "watch:ts": "npx tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "semantic-release": "semantic-release"
  },
  "name": "godo-api",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
